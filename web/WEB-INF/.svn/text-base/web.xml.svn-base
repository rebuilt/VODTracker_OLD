<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>VideoDataService</servlet-name>
        <servlet-class>org.homelinux.server.VideoDataServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VideoUpdateService</servlet-name>
        <servlet-class>org.homelinux.server.VideoUpdateServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VideoDataService</servlet-name>
        <url-pattern>/org.homelinux.starcraft/videodataservice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoUpdateService</servlet-name>
        <url-pattern>/org.homelinux.starcraft/videoupdateservice</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>welcomeGWT.html</welcome-file>
        </welcome-file-list>
    </web-app>
